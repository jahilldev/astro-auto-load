---
import WaterfallChild2 from './WaterfallChild2.astro';

const start = Date.now();
await new Promise((resolve) => setTimeout(resolve, 100));
const duration = Date.now() - start;
---

<div class="waterfall-child3" data-start={start} data-duration={duration}>
  <p>WaterfallChild3: standard async, took {duration}ms</p>
  <WaterfallChild2 />
</div>
